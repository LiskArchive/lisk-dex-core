{
	"name": "lisk-dex-core",
	"version": "1.0.0-beta.0",
	"description": "Lisk DEX Core",
	"author": "emilio",
	"license": "ISC",
	"keywords": [
		"blockchain",
		"lisk",
		"nodejs",
		"javascript",
		"typescript"
	],
	"homepage": "",
	"repository": {},
	"engines": {
		"node": ">=18.12.0 <=18",
		"npm": ">=8.1.0"
	},
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*.{js,ts}'",
		"prepack": "oclif manifest && oclif readme --multi --dir=docs/commands && npm shrinkwrap && npm prune --production && npm shrinkwrap",
		"prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo; rm -f npm-shrinkwrap.json",
		"start": "echo Run \"./bin/run start\" to start the app",
		"build": "tsc",
		"test": "jest --passWithNoTests",
		"test:coverage": "jest --passWithNoTests --coverage=true --coverage-reporters=json",
		"test:ci": "jest --passWithNoTests --coverage=true --coverage-reporters=json --verbose",
		"version": "oclif readme --multi --dir=docs/commands && git add README.md docs",
		"prepublishOnly": "npm ci && npm run lint && npm run build"
	},
	"bin": {
		"lisk-dex-core": "./bin/run"
	},
	"lisk": {
		"addressPrefix": "lsk"
	},
	"oclif": {
		"bin": "lisk-dex-core",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-version"
		],
		"additionalHelpFlags": [
			"-h"
		],
		"additionalVersionFlags": [
			"-v"
		],
		"topicSeparator": " ",
		"topics": {
			"block": {
				"description": "Commands relating to lisk-dex-core blocks."
			},
			"blockchain": {
				"description": "Commands relating to lisk-dex-core blockchain data."
			},
			"console": {
				"description": "lisk-dex-core interactive REPL session to run commands."
			},
			"config": {
				"description": "Commands relating to lisk-dex-core node configuration."
			},
			"keys": {
				"description": "Commands relating to lisk-dex-core key generation."
			},
			"generator": {
				"description": "Commands relating to lisk-dex-core block generator."
			},
			"hash-onion": {
				"description": "Create hash onions to be used by the forger."
			},
			"system": {
				"description": "Commands relating to lisk-dex-core node."
			},
			"passphrase": {
				"description": "Commands relating to lisk-dex-core passphrases."
			},
			"endpoint": {
				"description": "Commands relating to lisk-dex-core endpoint."
			},
			"sdk": {
				"description": "Commands relating to Lisk SDK development."
			},
			"transaction": {
				"description": "Commands relating to lisk-dex-core transactions."
			}
		}
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/dist",
		"/config",
		"/docs"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@liskhq/lisk-framework-dashboard-plugin": "0.3.0-rc.0",
		"@liskhq/lisk-framework-faucet-plugin": "0.3.0-rc.0",
		"@liskhq/lisk-framework-monitor-plugin": "0.4.0-rc.0",
		"@liskhq/lisk-framework-forger-plugin": "0.4.0-rc.0",
		"@liskhq/lisk-framework-report-misbehavior-plugin": "0.4.0-rc.0",
		"@liskhq/lisk-framework-chain-connector-plugin": "0.1.0-rc.0",
		"@oclif/core": "1.20.4",
		"@oclif/plugin-autocomplete": "1.3.6",
		"@oclif/plugin-help": "5.1.19",
		"@oclif/plugin-version": "1.1.3",
		"axios": "0.21.2",
		"fs-extra": "11.1.0",
		"inquirer": "8.2.5",
		"lisk-commander": "6.0.0-rc.0",
		"lisk-sdk": "6.0.0-rc.0",
		"tar": "6.1.11",
		"tslib": "2.4.1",
		"@liskhq/lisk-chain": "0.5.0-rc.0",
		"@liskhq/lisk-codec": "0.4.0-rc.0",
		"@liskhq/lisk-cryptography": "4.0.0-rc.0",
		"@liskhq/lisk-db": "0.3.8",
		"@liskhq/lisk-utils": "0.4.0-rc.0",
		"@liskhq/lisk-validator": "0.8.0-rc.0",
		"@oclif/command": "1.8.20",
		"@oclif/config": "1.18.8",
		"bigint-buffer": "^1.1.5",
		"bunyan": "1.8.15",
		"lisk-framework": "0.11.0-rc.0"
	},
	"devDependencies": {
		"@oclif/test": "2.2.12",
		"@types/fs-extra": "9.0.13",
		"@types/node": "18.15.3",
		"@types/tar": "6.1.3",
		"@types/jest": "29.2.3",
		"@types/jest-when": "3.5.2",
		"@typescript-eslint/eslint-plugin": "5.44.0",
		"@typescript-eslint/parser": "5.44.0",
		"eslint": "8.28.0",
		"eslint-config-lisk-base": "2.0.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jest": "27.1.6",
		"globby": "10.0.2",
		"husky": "4.3.8",
		"jest": "29.3.1",
		"jest-extended": "3.2.0",
		"jest-when": "3.5.2",
		"oclif": "3.2.31",
		"lint-staged": "10.2.11",
		"prettier": "2.8.0",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "5.0.2"
	}
}
